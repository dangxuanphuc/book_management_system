<% provide :title, t(".link1") %>
<ol class="breadcrumb text-left">
  <li><%= link_to t("layouts.sidebar.dashboard" ),
      admin_root_path, method: :get %></li>
  <li class="active"><%= t ".link1" %></li>
</ol>
<h1 class="section-title" id="services">
  <span><%= t ".page_title" %></span>
</h1>
<div class="row">
  <div class="col-xs-12">
    <% flash.each do |key, value| %>
      <div class="flash-message <%= key %>">
        <%= value %>
      </div>
    <% end %>
    <div class="search-form" id="author">
      <%= render "author", authors: @authors %>
    </div>
  </div>
</div>
